[alias]
 ci = commit
 st = status --short
 co = checkout
 up = pull --rebase
 lg = log --oneline
 last = log -1 HEAD
 c = log -p -w --reverse @{u}..
 b = branch -vv
 fix = commit --amend
[color]
 diff = auto
 status = auto
 branch = auto
[push]
 default = simple
